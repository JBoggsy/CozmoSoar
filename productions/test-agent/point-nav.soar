sp {test-agent*propose*go-to-paperclip
   (state <s> ^name test-agent-point-nav
              ^io.input-link <in>
             -^visited 1)
   (<in> ^object <obj>)
   (<obj> ^cube_id 1
          ^object_id <id>
          ^distance > 125)
-->
   (<s> ^operator <op> + =)
   (<op> ^name go-to-object
         ^actions.go-to-object <a>)
   (<a> ^target_object_id <id>)
}

sp {test-agent*propose*search-for-lamp
   (state <s> ^name test-agent-point-nav
              ^io.input-link <in>
              ^visited 1
             -^visited 2)
   (<in> -^object.cube_id 2)
-->
   (<s> ^operator <op> + =)
   (<op> ^name turn-in-place
         ^actions true
         ^angle 45
         ^speed 15)
}

